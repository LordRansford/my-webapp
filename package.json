{
  "name": "my-webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "build": "node -e \"const fs=require('fs');const p='.next/lock';if(fs.existsSync(p)) fs.rmSync(p,{force:true});\" && next build --webpack",
    "start": "next start --hostname 0.0.0.0 --port 3000",
    "lint": "eslint && npm run lint:cpd && npm run lint:templates",
    "lint:cpd": "node cpd-quality.mjs",
    "lint:notes": "node scripts/notes_lint.mjs",
    "notes:lint": "node scripts/notes_lint.mjs",
    "quality:content:ai-beginner": "node scripts/content-quality.mjs content/notes/ai/beginner.mdx",
    "quality:tools": "node scripts/tool-quality.mjs",
    "quality:ui": "node scripts/ui-quality.mjs",
    "quality:all": "npm run quality:ui && npm run quality:tools && npm run quality:content:ai-beginner && npm run quality:content:ai-advanced",
    "lint:dashboards": "node scripts/dashboard-quality.mjs && node scripts/dashboard-allowlist.mjs",
    "lhci": "lhci autorun",
    "quality:content:ai-advanced": "node scripts/content-quality-advanced.mjs src/pages/ai/advanced.js",
    "lint:templates": "node scripts/check-template-definitions.mjs"
  },
  "dependencies": {
    "@runno/runtime": "^0.10.0",
    "@tensorflow-models/mobilenet": "^2.1.1",
    "@tensorflow/tfjs": "^4.22.0",
    "@types/better-sqlite3": "^7.6.13",
    "@vercel/speed-insights": "^1.3.1",
    "better-sqlite3": "^12.5.0",
    "docx": "^9.5.1",
    "file-saver": "^2.0.5",
    "framer-motion": "^11.0.3",
    "gray-matter": "^4.0.3",
    "html2canvas": "^1.4.1",
    "katex": "^0.16.27",
    "lucide-react": "^0.469.0",
    "next": "16.0.10",
    "next-mdx-remote": "^5.0.0",
    "papaparse": "^5.5.3",
    "pdf-lib": "^1.17.1",
    "better-sqlite3": "^11.7.0",
    "react": "19.2.3",
    "react-dom": "18.3.1",
    "reading-time": "^1.5.0",
    "recharts": "^2.15.4",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-katex": "^7.0.1",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "sanitize-html": "^2.17.0",
    "sql.js": "^1.13.0",
    "stripe": "^20.1.0",
    "xlsx": "^0.18.5",
    "zod": "^4.2.1",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@lhci/cli": "^0.13.0",
    "@tailwindcss/postcss": "^4",
    "@types/better-sqlite3": "^7.6.11",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "tailwindcss": "^4",
    "typescript": "^5.9.3"
  }
}
