{
  "name": "my-webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "build:content-index": "node scripts/build-content-index.mjs",
    "build:tools-index": "node scripts/build-tools-index.mjs",
    "build": "npm run quality:compute && npm run quality:credits && npm run enforce:credit-patterns && npm run validate:tool-contracts && npm run build:content-index && npm run build:tools-index && node -e \"const fs=require('fs');const p='.next/lock';if(fs.existsSync(p)) fs.rmSync(p,{force:true});\" && next build",
    "validate:tool-contracts": "node scripts/validate-tool-contracts.mjs",
    "validate:env": "node scripts/validate-env.mjs",
    "start": "next start --hostname 0.0.0.0 --port 3000",
    "postinstall": "npx prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "lint": "eslint && npm run lint:auth && npm run lint:monetization && npm run lint:stripe && npm run lint:access && npm run lint:cpd && npm run lint:templates",
    "lint:auth": "node scripts/auth-guards.mjs",
    "lint:monetization": "node scripts/monetization-guards.mjs",
    "lint:stripe": "node scripts/stripe-guards.mjs",
    "lint:access": "node scripts/access-guards.mjs",
    "lint:cpd": "node cpd-quality.mjs",
    "lint:notes": "node scripts/notes_lint.mjs",
    "notes:lint": "node scripts/notes_lint.mjs",
    "quality:content:ai-beginner": "node scripts/content-quality.mjs content/notes/ai/beginner.mdx",
    "quality:tools": "node scripts/tool-quality.mjs",
    "quality:ui": "node scripts/ui-quality.mjs",
    "quality:assistants": "node scripts/assistants-quality.mjs",
    "quality:compute": "node scripts/compute-quality.mjs",
    "quality:credits": "node scripts/credits-quality.mjs",
    "enforce:credit-patterns": "node scripts/enforce-credit-patterns.mjs",
    "quality:stage1": "node scripts/stage1-quality.mjs",
    "quality:all": "npm run quality:ui && npm run quality:tools && npm run quality:content:ai-beginner && npm run quality:content:ai-advanced",
    "lint:dashboards": "node scripts/dashboard-quality.mjs && node scripts/dashboard-allowlist.mjs",
    "lhci": "lhci autorun",
    "quality:content:ai-advanced": "node scripts/content-quality-advanced.mjs src/pages/ai/advanced.js",
    "build:mentor-embeddings": "node scripts/build-mentor-embeddings.mjs",
    "validate:mentor-safety": "node scripts/validate-mentor-safety.mjs",
    "lint:templates": "node scripts/check-template-definitions.mjs",
    "test:security": "node scripts/secret-scan.mjs",
    "test:data": "node --test tests/**/*.test.mjs",
    "test:mentor": "npm run build:content-index && node --test tests/mentor-*.test.mjs",
    "test:contracts": "node --test tests/contracts/*.test.mjs",
    "test:e2e": "node -e \"const fs=require('fs');const p='.next/dev/lock';if(fs.existsSync(p)) fs.rmSync(p,{force:true});\" && playwright test -c playwright.config.mjs",
    "test:ux": "node scripts/ux-check.mjs",
    "worker:tick": "node scripts/worker.mjs",
    "news:ingest": "npx tsx scripts/news-ingest/main.ts",
    "runner:dev": "docker compose -f docker-compose.runner.yml up --build",
    "runner:down": "docker compose -f docker-compose.runner.yml down"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "@runno/runtime": "^0.10.0",
    "@stripe/react-stripe-js": "^2.4.0",
    "@stripe/stripe-js": "^2.4.0",
    "@tensorflow-models/mobilenet": "^2.1.1",
    "@tensorflow/tfjs": "^4.22.0",
    "@types/xml2js": "^0.4.14",
    "@vercel/analytics": "^1.6.1",
    "@vercel/blob": "^2.0.0",
    "@vercel/speed-insights": "^1.3.1",
    "docx": "^9.5.1",
    "file-saver": "^2.0.5",
    "framer-motion": "^11.0.3",
    "gray-matter": "^4.0.3",
    "html2canvas": "^1.4.1",
    "katex": "^0.16.27",
    "lucide-react": "^0.469.0",
    "mermaid": "^10.9.5",
    "next": "16.0.10",
    "next-auth": "^4.24.13",
    "next-mdx-remote": "^5.0.0",
    "nodemailer": "^7.0.12",
    "papaparse": "^5.5.3",
    "pdf-lib": "^1.17.1",
    "pdfkit": "^0.17.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "reading-time": "^1.5.0",
    "recharts": "^2.15.4",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-katex": "^7.0.1",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "sanitize-html": "^2.17.0",
    "server-only": "^0.0.1",
    "sql.js": "^1.13.0",
    "stripe": "^20.1.0",
    "svg-to-pdfkit": "^0.1.8",
    "xlsx": "^0.18.5",
    "xml2js": "^0.6.2",
    "zod": "^4.2.1",
    "zustand": "^4.5.7",
    "prisma": "^6.1.0"
  },
  "devDependencies": {
    "@lhci/cli": "^0.13.0",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@types/better-sqlite3": "^7.6.11",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"
  }
}
